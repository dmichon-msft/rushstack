{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/common/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": false,
			"args": [
			],
			"cwd": "${workspaceRoot}/gulp-core-build",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Rush Debug",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/apps/rush-lib/lib/start.js",
			"stopOnEntry": true,
			"args": [
				"update",
				"--variant",
				"test"
			],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
        "--debug"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"sourceMaps": true
		},
		{
			"name": "Debug Heft-Jest",
			"type": "node",
			"request": "launch",
			"program": "./node_modules/@rushstack/heft/lib/start.js",
			"stopOnEntry": false,
			"args": [
				"--debug",
				"test"
			],
			"cwd": "${workspaceRoot}/heft-plugins/heft-jest-plugin",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
        "--inspect"
			],
			"env": {
				"NODE_ENV": "development"
			},
      "skipFiles": ["<node_internals>/**"],
			"outFiles": ["${workspaceRoot}/heft-plugins/heft-jest-plugin/lib"],
			"sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}
